<div class="login">
  <h1>Login</h1>
  <%= form_tag '/login' do %>
  <div class="entradas">
    <%= label_tag :email%>:
    <%= text_field_tag :email %>
    <br><br>
    <%= label_tag :password%>:
    <%= password_field_tag :password%>
  </div>
  <br><br>
  <%= submit_tag "Login"%>
  <%end%>
</div>
